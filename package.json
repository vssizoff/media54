{
  "name": "media54",
  "version": "1.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build:web": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build:web && electron-builder --dir",
    "build:win": "npm run build:web && electron-builder --win --publish never",
    "build:mac": "npm run build:web && electron-builder --mac",
    "build:linux": "npm run build:web && electron-builder --linux",
    "build": "npm run build:web && electron-builder --linux --win --publish never"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@primevue/themes": "^4.3.3",
    "music-metadata": "^11.6.0",
    "primevue": "^4.3.3",
    "vue-router": "^4.5.1",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^22.14.1",
    "@types/nodemailer": "^6.4.17",
    "@vitejs/plugin-vue": "^5.2.3",
    "electron": "^35.1.5",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.1.0",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vue": "^3.5.13",
    "vue-eslint-parser": "^10.1.3",
    "vue-tsc": "^2.2.8"
  }
}